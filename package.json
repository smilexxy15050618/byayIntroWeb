  "scripts": {
    "start:dev": "cross-env NODE_ENV=development PORT=3001 nodemon server.js",
    "build:dev": "next build",
    "build": "NODE_ENV=production next build",
    "export": "next export",
    "analyze": "NODE_ENV=production ANALYZE=true next build",
    "start": "cross-env NODE_ENV=production PORT=6003 nodemon server.js",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "ts:check": "tsc --noEmit",
    "lint": " eslint src --quiet --ext .tsx,.ts",
    "format-prettier": "prettier --write '**/*.{css,gql,graphql,html,js,jsx,json,less,md,mdx,scss,ts,tsx,vue}'",
    "format-eslint": "eslint src --quiet --cache --ext .tsx,.ts,.js --fix",
    "tar-src": "tar zcvf /tmp/node.tar.gz src static server.js .next package.json config.env.js next.config.js pm2.test.config.js pm2.production.config.js node_modules",
    "tar": "npm run tar-src && mv /tmp/node.tar.gz .",
    "prepare": "husky install"
  },
